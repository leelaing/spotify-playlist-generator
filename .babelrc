{
  "presets": [
    "react",
    "env",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src/js"],
      "alias": {
          "components": "/components",
          "utils": "/utils",
          "config": "/config"
      }
    }],
    "react-hot-loader/babel"
  ]
}
